<div id="map"
     style="width: 100%; height: 100vh;"
     data-markers="<%= @markers.to_json %>"
     data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>">
</div>


<div id="main">
  <div id="top">
  </div>
  <div id="bottom">
    <div id="lists">
      <div class="sidenav">
        <% @resources.each do |resource| %>
        <div class= "card m-2 p-2">
          <div class= "resource_infos">
              <p class="name"><%= resource.name %></p>
          </div>
        </div>
        <% end %>
      </div>
    </div>
  </div>
</div>


